@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h3>Список ролей</h3>
    <table>
        <tr class="header">
            <td><p>Пользователь</p></td>
            <td><p>Роль</p></td>
        </tr>
        @foreach (var b in ViewBag.UserRoles)
            {
            <tr>
                <td><p>@b.UserName</p></td>
                <td><p>@b.Roles.Name</p></td>
            </tr>
        }
    </table>
</div>
